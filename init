#!/usr/bin/env bash
MyHome = $(dirname $0)

if [[ ! -d ${MyHome}/venv ]]
then
  echo First time init of aquaPI development env.
  read -p CR to continue / Ctrl-C dummy
  cd ${MyHome}
  . ./venv/bin/activate
  pip install Flask sseclient
else
  echo Activate aquaPI development env.
  cd ${MyHome}
  . ./venv/bin/activate
  fi
